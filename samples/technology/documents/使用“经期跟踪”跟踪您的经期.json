        "20": {
            "data": "您还可以添加可能影响月经周期时间和时长的因素，例如怀孕。",
            "type": "sequence",
            "pid": "19"
        },
        "21": {
            "data": "在iPhone上，打开“健康”App，然后轻点“浏览”标签页。",
            "type": "sequence-step",
            "pid": "20"
        },
        "22": {
            "data": "轻点“经期跟踪”。",
            "type": "sequence-step",
            "pid": "20"
        },
